    <main class="container py-4">
        <h1 class="mb-4 text-primary">Statistiques des Mdecins & Consultations</h1>
        <div class="row mb-4">
            <div class="col-md-4 mb-3">
                <div class="card shadow-sm border-0">
                    <div class="card-body text-center">
                        <h5 class="card-title text-success">Total Mdecins</h5>
                        <h2 class="display-5 fw-bold">{{ totalDoctors }}</h2>
                        <p class="text-muted">Mdecins actifs dans le systme</p>
                    </div>
                </div>
            </div>
            <div class="col-md-4 mb-3">
                <div class="card shadow-sm border-0">
                    <div class="card-body text-center">
                        <h5 class="card-title text-primary">Total Rendez-vous</h5>
                        <h2 class="display-5 fw-bold">{{ totalRendezVous }}</h2>
                        <p class="text-muted">Rendez-vous enregistrs</p>
                    </div>
                </div>
            </div>
            <div class="col-md-4 mb-3">
                <div class="card shadow-sm border-0">
                    <div class="card-body text-center">
                        <h5 class="card-title text-info">Total Fiches Mdicales</h5>
                        <h2 class="display-5 fw-bold">{{ totalFiches }}</h2>
                        <p class="text-muted">Fiches mdicales cres</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="row mb-4">
            <div class="col-md-6 mb-3">
                <div class="card shadow-sm border-0">
                    <div class="card-header bg-primary text-white">Rpartition des Rendez-vous par Statut</div>
                    <div class="card-body">
                        <div id="pieRdvStatut" style="height:300px;"></div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 mb-3">
                <div class="card shadow-sm border-0">
                    <div class="card-header bg-success text-white">Rpartition des Fiches Mdicales par Diagnostic</div>
                    <div class="card-body">
                        <div id="pieFicheDiagnostic" style="height:300px;"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row mb-4">
            <div class="col-md-12 mb-3">
                <div class="card shadow-sm border-0">
                    <div class="card-header bg-info text-white">Consultations par Mdecin</div>
                    <div class="card-body">
                        <div id="barDoctorsConsultations" style="height:350px;"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row mb-4">
            <div class="col-md-6 mb-3">
                <div class="card shadow-sm border-0">
                    <div class="card-header bg-warning text-dark">Rendez-vous par Mois</div>
                    <div class="card-body">
                        <div id="lineRdvMonth" style="height:300px;"></div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 mb-3">
                <div class="card shadow-sm border-0">
                    <div class="card-header bg-secondary text-white">Fiches Mdicales par Mois</div>
                    <div class="card-body">
                        <div id="lineFicheMonth" style="height:300px;"></div>
                    </div>
                </div>
            </div>
        </div>
        <script>
            document.addEventListener('DOMContentLoaded', function() {
                // Pie chart for rendez-vous status
                var pieRdvStatut = new ApexCharts(document.querySelector("#pieRdvStatut"), {
                    chart: { type: 'pie', height: 300 },
                    series: {{ rdvStatutCounts|json_encode|raw }},
                    labels: {{ rdvStatutLabels|json_encode|raw }},
                    colors: ['#0d6efd', '#198754', '#ffc107', '#dc3545'],
                    legend: { position: 'bottom' }
                });
                pieRdvStatut.render();
                // Pie chart for fiche diagnostics
                var pieFicheDiagnostic = new ApexCharts(document.querySelector("#pieFicheDiagnostic"), {
                    chart: { type: 'pie', height: 300 },
                    series: {{ ficheDiagnosticCounts|json_encode|raw }},
                    labels: {{ ficheDiagnosticLabels|json_encode|raw }},
                    colors: ['#17a2b8', '#6610f2', '#fd7e14', '#20c997'],
                    legend: { position: 'bottom' }
                });
                pieFicheDiagnostic.render();
                // Bar chart for consultations per doctor
                var barDoctorsConsultations = new ApexCharts(document.querySelector("#barDoctorsConsultations"), {
                    chart: { type: 'bar', height: 350 },
                    series: [{ name: 'Consultations', data: {{ doctorsConsultationsCounts|json_encode|raw }} }],
                    xaxis: { categories: {{ doctorsConsultationsLabels|json_encode|raw }} },
                    colors: ['#0dcaf0'],
                    legend: { position: 'top' }
                });
                barDoctorsConsultations.render();
                // Line chart for rendez-vous per month
                var lineRdvMonth = new ApexCharts(document.querySelector("#lineRdvMonth"), {
                    chart: { type: 'line', height: 300 },
                    series: [{ name: 'Rendez-vous', data: {{ rdvMonthCounts|json_encode|raw }} }],
                    xaxis: { categories: {{ rdvMonthLabels|json_encode|raw }} },
                    colors: ['#ffc107'],
                    legend: { position: 'top' }
                });
                lineRdvMonth.render();
                // Line chart for fiches mdicales per month
                var lineFicheMonth = new ApexCharts(document.querySelector("#lineFicheMonth"), {
                    chart: { type: 'line', height: 300 },
                    series: [{ name: 'Fiches Mdicales', data: {{ ficheMonthCounts|json_encode|raw }} }],
                    xaxis: { categories: {{ ficheMonthLabels|json_encode|raw }} },
                    colors: ['#6c757d'],
                    legend: { position: 'top' }
                });
                lineFicheMonth.render();
            });
        </script>
